{
  "name": "interflux",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for interflux goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve --environment=development",
    "serve": "ember serve --environment=development",
    "build": "ember build --environment=production",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "setup": "remote/setup.sh",
    "deploy": "remote/deploy.sh",
    "build-ios": "IOS_BUILD=true corber build --platform=ios --environment=production",
    "build-android": "ANDROID_BUILD=true corber build --platform=android --environment=production",
    "start-ios": "IOS_BUILD=true corber start --platform=ios",
    "android": "ANDROID_BUILD=true ember s --port=4200 --live-reload-port=0 --output-path=dist-android/",
    "start-android": "ANDROID_BUILD=true corber start --platform=android",
    "format": "prettier --write"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.26.1",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-data": "~3.27.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-template-lint": "^3.11.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "ember-cli-autoprefixer": "^1.0.3",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-head": "^1.0.0",
    "ember-cli-sass": "^10.0.1",
    "ember-href-to": "^4.1.0",
    "ember-mapbox-gl": "^0.16.0",
    "ember-responsive": "^4.0.1",
    "ember-truth-helpers": "^3.0.0",
    "git-rev-sync": "^3.0.1",
    "mapbox-gl": "^1.0.0",
    "sass": "^1.33.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "14.17.0",
    "yarn": "1.22.10"
  }
}
