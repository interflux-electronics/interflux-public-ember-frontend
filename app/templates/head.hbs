{{! https://ogp.me/ }}
{{! https://developers.facebook.com/docs/sharing/webmasters/ }}
{{! https://developers.facebook.com/docs/sharing/best-practices#images }}

<title>{{this.model.title}}</title>
<meta name='description' content='{{this.model.description}}' />
<meta property='og:type' content={{this.model.type}} />
<meta property='og:locale' content={{this.model.locale}} />
<meta property='og:url' content='{{this.model.url}}' />
<meta property='og:title' content='{{this.model.title}}' />
<meta property='og:description' content='{{this.model.description}}' />
{{#if this.model.imageURL}}
  <meta property='og:image' content={{this.model.imageURL}} />
  <meta property='og:image:secure_url' content={{this.model.imageURL}} />
  <meta property='og:image:type' content={{this.model.imageMime}} />
  {{#if this.model.imageWidth}}
    <meta property='og:image:width' content={{this.model.imageWidth}} />
  {{/if}}
  {{#if this.model.imageHeight}}
    <meta property='og:image:height' content={{this.model.imageHeight}} />
  {{/if}}
{{/if}}
<meta name='robots' content={{this.model.robot}} />
<meta name='copyright' content='© since 1980 by Interflux Electronics ®' />
<meta name='built-by' content='Jan Werkhoven, j.werkhoven@interflux.com' />
<meta name='built-with' content='Ember Octane, https://emberjs.com/' />
<meta name='deploy-time' content={{this.model.buildTimestamp}} />
<meta name='environment' content={{this.model.environment}} />
<meta
  name='git-repo'
  content='https://github.com/interflux-electronics/interflux-public-ember-frontend'
/>
<meta name='git-branch' content={{this.model.gitBranch}} />
<meta name='git-revision' content={{this.model.gitRevision}} />
<meta name='theme-color' content='#23578c' />
<link
  rel='icon'
  type='image/png'
  href='{{env 'cdnHost'}}/images/favicons/interflux-blue-favicon@228x228.png'
  sizes='228x228'
/>
<link rel='canonical' href='{{this.model.url}}' />

