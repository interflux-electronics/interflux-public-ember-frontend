<Header::Desktop::Menu @isOpen={{@isOpen}}>
  <h2>{{t 'ui.header.processes.title'}}</h2>
  <div class='links'>
    {{#each this.processes as |process|}}
      <Button @route='products.subset' @model={{process.routeModel}}>
        <img
          src='{{env "cdnHost"}}/images/icons/{{process.icon}}.svg'
          role='presentation'
          alt=''
        />
        <span>{{process.label}}<Svg::ArrowRight /></span>
      </Button>
    {{/each}}
  </div>
</Header::Desktop::Menu>